{
  "name": "@scottie/simple-vue-timeline",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/@scottie/simple-vue-timeline.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.ts"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name simple-vue-timeline src/main2.js",
    "build-declaration": "rm -f index.d.ts && tsc -p declaration.tsconfig.json",
    "build-bundle-prod": "vue-cli-service build --target lib --name @scottie/simple-vue-timeline ./src/components/index.js --mode production"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "bootstrap-vue": "^2.5.0",
    "core-js": "^2.6.11",
    "moment": "^2.24.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.8.3",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "prettier": "^1.19.1",
    "sass-loader": "^7.3.1",
    "typescript": "^3.8.3",
    "vue-cli-plugin-bootstrap-vue": "^0.4.0",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ],
    "{,src/**/}*.{md,json,js,ts,css,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
